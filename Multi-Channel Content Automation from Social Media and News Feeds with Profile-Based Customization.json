    {
      "parameters": {
        "promptType": "define",
        "text": "=Analyze and summarize this content:\n\nTitle: {{ $json.title }}\nContent: {{ $json.content || $json.description || $json.text }}\nSource: {{ $json.source }}\nURL: {{ $json.link || $json.url }}",
